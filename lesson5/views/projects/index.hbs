<h1>{{title}}</h1>
<a href="/projects/add" class="btn btn-primary">Add a new project</a>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Due Date</th>
            <th>Course</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        {{#each projects}}
        <tr>
            <th>{{this.name}}</th>
            <th>{{this.dueDate}}</th>
            <th>{{this.course}}</th>
            <th>{{this.status}}</th>
        </tr>
        {{/each}}
    </tbody>
</table>
<p>Weird test</p>