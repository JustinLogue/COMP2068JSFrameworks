<h1>{{title}}</h1>
<a href="/projects/add" class="btn btn-primary">Add a new project</a>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Due Date</th>
            <th>Course</th>
            <th>Status</th>
            <th>Update</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each dataset}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{toShortDate this.dueDate}}</td>
            <td>{{this.course}}</td>
            <td>{{this.status}}</td>
            <td> 
                <a href="/projects/edit/{{this._id}}" class="btn btn-info">Update</a>
                &nbsp;
                <a href="/projects/delete/{{this._id}}" class="btn btn-danger" onclick="confirmDeletion()">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<p>Weird test</p>