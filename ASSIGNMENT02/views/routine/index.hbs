<h1>{{title}}</h1>
{{#if user}}
    <a href="/routines/add" class="btn btn-primary">Add a new project</a>
{{/if}}
<h1>Work in progess!</h1>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Hinge</th>
            <th>Push</th>
            <th>Squat</th>
            <th>Pull</th>
            <th>Cardio</th>
            
        </tr>
    </thead>
    <tbody>
        {{#each dataset}}
        <tr>
            <td>{{this.name}}</td>
            <td>Activity: {{this.hingeActivity}} Sets: {{this.hingeSets}} Reps:{{this.HingeReps}} {{#if this.Weight}} Weight: {{/if}}{{}}</td>
            <td>{{this.course}}</td>
            <td>{{this.status}}</td>
            <td> 
                {{#if ../user}}
                <a href="/projects/edit/{{this._id}}" class="btn btn-info">Update</a>
                &nbsp;
                <a href="/projects/delete/{{this._id}}" class="btn btn-danger" onclick="confirmDeletion()">Delete</a>
                {{/if}}
            </td>
            
        </tr>
        {{/each}}
    </tbody>
</table>
<p>Weird test</p>